{
  "name": "@tg/lottery-h5",
  "type": "module",
  "version": "0.0.8",
  "packageManager": "pnpm@8.6.7",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./core": {
      "import": "./core/index.ts"
    }
  },
  "scripts": {
    "build": "vite build --mode test",
    "build-fat1": "sh ./scripts/build.sh fat",
    "build:test": "sh ./scripts/build.sh",
    "build:fat": "vite build --mode fat",
    "build:prod": "sh ./scripts/build-prod.sh",
    "build:production": "vite build --mode production",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 vite --port 4444",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "sizecheck": "npx vite-bundle-visualizer",
    "generate:webp": "sh ./scripts/genImg.sh",
    "add:lang": "sh ./scripts/addLang.sh",
    "addPrePush": "sh ./scripts/addPrePush.sh",
    "createBranch": "node ./scripts/createBranch.js"
  },
  "dependencies": {
    "@tg/apis": "workspace:^",
    "@tg/bccomponents": "workspace:^",
    "@tg/hooks": "workspace:^",
    "@tg/icons": "workspace:*",
    "@tg/shared-router": "workspace:^",
    "@tg/stores": "workspace:^",
    "@tg/tailwindcss": "workspace:^",
    "@tg/types": "workspace:^",
    "@tg/utils": "workspace:^",
    "@tg/vue-i18n": "workspace:^"
  }
}
